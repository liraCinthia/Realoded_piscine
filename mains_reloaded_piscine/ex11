#include <stdio.h>

void    ft_div_mod(int a, int b, int *div, int *mod)
{
        *div = a / b;
        *mod = a % b;
}

int main()
{
        int a = 5;
        int b = 4;

        int div;
        int mod;

        ft_div_mod(a, b, &div, &mod);
        printf("valor de a:%d valor de b:%d\n", a, b);
        printf("valor da div:%d valor da mod:%d\n", div, mod);
        return (0);
}

