#include <stdio.h>

int     ft_sqrt(int nb)
{
        int     i;

        i = 0;
        while (i*i != nb)
        {
                if (i > nb || i < 0)
                {
                        return (0);
                }
                i++;
        }
        return (i);
}

int main()
{
        printf("%d\n", ft_sqrt(9));
        return (0);
}

