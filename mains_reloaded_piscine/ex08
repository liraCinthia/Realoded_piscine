#include <unistd.h>
#include <stdio.h>

void    ft_putchar(char c)
{
	write(1, &c, 1);
}

void    ft_is_negative(int n)
{
        if (n >= 0)
        {
                ft_putchar('P');
                n++;
        } else
        {
                ft_putchar('N');
        }
}

int main()
{
        ft_is_negative(-2);
        return (0);
}

