#include <stdio.h>

int     ft_iterative_factorial(int nb)
{
        int     r;

        r = 1;
        if (nb < 0)
        {
                return (0);
        }
        while (nb >= 1)
        {
                r *= nb;
                nb--;
        }
        return (r);
}

int main()
{
        printf("o numero fatorial e:%d\n", ft_iterative_factorial(3));
        return (0);
}

